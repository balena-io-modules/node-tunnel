{
  "name": "node-tunnel",
  "version": "2.1.0",
  "description": "Http tunneling proxy",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:resin-io/node-tunnel"
  },
  "author": "Aleksis Brezas <abresas@resin.io>",
  "contributors": [
    "Will Boyce <will@resin.io>"
  ],
  "scripts": {
    "lint": "resin-lint --typescript src/ test/ typings/",
    "lint:fix": "prettier --config ./node_modules/resin-lint/config/.prettierrc --write {src,test,typings}/*.ts {src,test,typings}/**/*.ts",
    "check": "tsc --pretty --noEmit --diagnostics --project .",
    "build": "tsc --pretty --project ./tsconfig.dist.json",
    "test": "mocha --require ts-node/register/type-check test/index.ts",
    "prepublish": "require-npm4-to-publish",
    "prepare": "npm run build"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "basic-auth-parser": "0.0.2",
    "bluebird": "^3.5.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^8.10.45",
    "@types/request-promise": "^4.1.42",
    "chai": "^4.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^6.0.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "require-npm4-to-publish": "^1.0.0",
    "resin-lint": "^3.0.1",
    "ts-node": "^6.2.0",
    "typescript": "^3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --config ./node_modules/resin-lint/config/.prettierrc --write",
      "resin-lint --typescript --no-prettier",
      "git add"
    ]
  }
}
